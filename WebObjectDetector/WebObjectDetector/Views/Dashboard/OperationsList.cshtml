@model IEnumerable<WebObjectDetector.Dashboard.Models.OpencvOperations>

@{
    ViewData["Title"] = "OperationsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>OperationsList</h2>

<p>
    @Html.ActionLink("Create", "OperationsCreate", new { /* id=item.PrimaryKey */ })
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MessageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExperimentName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Offset_Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CurrentCount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxItems)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MessageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExperimentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Offset_Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CurrentCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxItems)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.ActionLink("Edit", "OperationsEdit", "Dashboard", new { item.Id /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "OperationsDetails", "Dashboard", new { item.Id/* id=item.PrimaryKey */ }, new { Area = "Dashboard" }) |
                @Html.ActionLink("Delete", "OperationsDelete", "Dashboard", new { item.Id /* id=item.PrimaryKey */ }, new { Area = "Dashboard" })
            </td>
        </tr>
}
    </tbody>
</table>
